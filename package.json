package.json{
  "name": "buggy-cars-qa-comprehensive-testing",
  "version": "1.0.0",
  "description": "Comprehensive QA Testing Suite for Buggy Cars Rating Application - 100+ Tests including Functional, Security, Performance, UI/UX, Database, and Error Handling",
  "scripts": {
    "test": "playwright test",
